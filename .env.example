# Database Configuration
DB_ROOT_PASSWORD=your_secure_root_password
DB_RADIUS_PASSWORD=your_secure_radius_password

# FreeRADIUS Database
RADIUS_DB_NAME=radius
RADIUS_DB_USER=radius

# Server Configuration
SERVER_NAME=localhost
